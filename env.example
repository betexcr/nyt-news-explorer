# Environment Variables for NYT News Explorer CDN

# NYT API Configuration
REACT_APP_NYT_API_KEY=your_nyt_api_key_here
REACT_APP_NYT_API_BASE_URL=https://api.nytimes.com

# CDN Configuration
REACT_APP_CDN_ENABLED=true
REACT_APP_CDN_BASE_URL=https://nyt.brainvaultdev.com

# Image Optimization
REACT_APP_IMAGE_OPTIMIZATION_ENABLED=true
REACT_APP_DEFAULT_IMAGE_QUALITY=80
REACT_APP_DEFAULT_IMAGE_WIDTH=800

# Performance Monitoring
REACT_APP_PERFORMANCE_MONITORING=true
REACT_APP_ANALYTICS_ENABLED=false

# Build Configuration
GENERATE_SOURCEMAP=false
PUBLIC_URL=https://nyt.brainvaultdev.com

# Netlify Configuration
NETLIFY_SITE_ID=your_netlify_site_id
NETLIFY_AUTH_TOKEN=your_netlify_auth_token

# Development
REACT_APP_ENVIRONMENT=production
REACT_APP_DEBUG=false

